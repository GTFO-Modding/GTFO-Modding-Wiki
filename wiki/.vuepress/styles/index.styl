kbd
  font-size 0.9rem
  color #1a1a1a

  .yuu-theme-dark &
    background-color #2f2f2f
    border-color #2a2a2a
    color #f3f3f3

.icon.outbound
  top -2px
  // left 1px

.yuu-theme-dark tr:nth-child(2n)
  background-color #222 !important

.navbar .logo
  border-radius: 2px  

mark
  $alphaFactor = 0.65

  color darken($textColor, 20)
  background-color alpha($accentColor, $alphaFactor)

  .yuu-theme-dark &
    color $darkTextColor

  .yuu-theme-blue &
    background-color alpha($blueAccentColor, $alphaFactor)

  .yuu-theme-red &
    background-color alpha($redAccentColor, $alphaFactor)

  .yuu-theme-purple &
    background-color alpha($purpleAccentColor, $alphaFactor)

unable()
  user-select: none
  -webkit-touch-callout: none
  -webkit-user-select: none
  -khtml-user-select: none
  -moz-user-select: none
  -ms-user-select: none

floatingBlock()
  display: block
  // color transparentify($textColor, 0.4)
  background-color transparentify(#e4e4e426, 3%)
  // box-shadow: rgba(100, 100, 111, 0.05) 0px 7px 29px 0px;

  .yuu-theme-dark &
    color $darkTextColor
    background-color: transparentify(#1a1a1a, 6%)

  margin 1rem 0
  padding .1rem 1.5rem
  border-radius 0.4rem

section .sidebar-group .collapsable > p > span .sidebar-heading
  unable();

.sidebar-group.is-sub-group > .sidebar-heading:not(.clickable)
  unable();
  opacity: 0.7 !important

code
  .yuu-theme-dark &
    color: #b5b5b5 !important

block
  floatingBlock()

  // background-color #f0f4f8
  // .title
  //   font-weight bold

  // &.right
  //   color transparentify($textColor, 0.4)
  //   font-size 0.9rem
  //   text-align right

.codespan
  color: #476582
  padding: 0px
  margin-block-start: 1em
  margin-block-end: 1em
  margin-inline-start: 0px
  margin-inline-end: 0px
  background-color: rgba(27,31,35,0.05)
  border-radius: 3px
  width: fit-content

  .yuu-theme-dark &
    color: #b5b5b5
    background-color: #2a2a2a

  p
    font-family: source-code-pro, Menlo, Monaco, Consolas, "Courier New", monospace
    padding: 0.25rem 0.5rem
    margin: 0
    line-height: 1.2
    font-size: 0.85em